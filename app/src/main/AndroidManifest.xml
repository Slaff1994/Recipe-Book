<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zane.RecipeApp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyTheme">
        <activity android:name="com.example.zane.RecipeApp.LandingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.zane.RecipeApp.AddRecipe">
            <uses-permission android:name="android.permission.CAMERA" />
        </activity>
        <activity android:name="com.example.zane.RecipeApp.ViewRecipe" />
        <activity android:name="com.example.zane.RecipeApp.Help" />
        <activity android:name="com.example.zane.RecipeApp.Home" />
        <activity android:name="com.example.zane.RecipeApp.SignUpActivity"></activity>
    </application>

</manifest>